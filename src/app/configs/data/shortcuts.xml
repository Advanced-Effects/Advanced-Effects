<?xml version='1.0' encoding='UTF-8'?>
<Shortcuts>
    <!-- File menu -->
    <SC>
        <key>project-new</key>
        <seq>Ctrl+N</seq>
    </SC>
    <SC>
        <key>project-open</key>
        <seq>Ctrl+O</seq>
    </SC>
    <SC>
        <key>asset-import</key>
        <seq>Ctrl+I</seq>
    </SC>
    <SC>
        <key>project-save</key>
        <seq>Ctrl+S</seq>
    </SC>
    <SC>
        <key>project-export</key>
        <seq>Ctrl+E</seq>
    </SC>
    <SC>
        <key>project-properties</key>
        <seq>Ctrl+Shift+I</seq>
    </SC>
    <SC>
        <key>quit</key>
        <seq>Ctrl+Q</seq>
    </SC>
    <!-- Edit menu -->
    <SC>
        <key>edit-copy</key>
        <seq>Ctrl+C</seq>
    </SC>
    <SC>
        <key>edit-paste</key>
        <seq>Ctrl+V</seq>
    </SC>
    <SC>
        <key>edit-cut</key>
        <seq>Ctrl+X</seq>
    </SC>
    <SC>
        <key>edit-undo</key>
        <seq>Ctrl+Z</seq>
    </SC>
    <SC>
        <key>edit-redo</key>
        <seq>Ctrl+Shift+Z</seq>
    </SC>
    <SC>
        <key>open-preferences</key>
        <seq>Ctrl+P</seq>
    </SC>
</Shortcuts>
